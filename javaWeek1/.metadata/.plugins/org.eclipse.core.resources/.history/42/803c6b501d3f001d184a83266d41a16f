
public class Main {

	public static void main(String[] args) {
		sayıBulmaca();

	}

	public static void sayıBulmaca() {

		int[] sayilar = new int[] { 1, 2, 5, 7, 9, 0 };
		int aranacak = 578;
		boolean varMı = false;

		for (int sayi : sayilar) {
			if (aranacak == sayi) {
				varMı = true;
				break;
			}
		}
		if (varMı == true) {
			mesajVer(7);
		} else {
			mesajVer(7);
		}
	}

	public static void mesajVer(int aranacak) {
		System.out.println("sayı mevcuttur:" + aranacak);
	}

}
